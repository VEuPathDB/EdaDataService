#%RAML 1.0 Library

types:
  # Type name determines the name of the Java class that will be generated.

  VisualizationRequestBase:
    additionalProperties: false
    properties:
      studyId: string
      filters:
        required: false
        type: API_Filter[]
      derivedVariables:
        required: false
        type: DerivedVariable[]

  StrataVariableDetails:
    type: VariableSpec
    properties:
      value: string
