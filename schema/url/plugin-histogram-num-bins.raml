#%RAML 1.0 Library

types:

  HistogramNumBinsPostRequest:
    type: HistogramPostRequest
    properties:
      config: HistogramNumBinsSpec

  HistogramNumBinsSpec:
    type: HistogramSpec
    properties:
      numBins:
        type: number
        required: false
        
  HistogramNumBinsPostResponse:
    type: HistogramPostResponse
    properties:
      numBins:
        type: number