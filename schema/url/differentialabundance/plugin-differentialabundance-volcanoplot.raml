#%RAML 1.0 Library

types:
  # Type name determines the name of the Java class that will be generated.

  DifferentialAbundanceVolcanoplotPostRequest:
    type: DataPluginRequestBase
    properties:
      computeConfig: DifferentialAbundanceComputeConfig
      config: DifferentialAbundanceVolcanoplotSpec

  # No variables, no missingness
  DifferentialAbundanceVolcanoplotSpec:
    type: object
    additionalProperties: false

  # dummy type for now to get things to compile
  DifferentialAbundanceComputeConfig:
    type: object
    additionalProperties: false
