#%RAML 1.0 Library

types:
  # Type name determines the name of the Java class that will be generated.

  FloatingMosaicPostRequest:
    type: VisualizationRequestBase
    properties:
      config: FloatingMosaicSpec

  FloatingMosaicSpec:
    additionalProperties: false
    properties:
      outputEntityId:
        type: string
      xAxisVariable:
        type: VariableSpec
      overlayVariable:
        type: VariableSpec
      overlayValues?:
        type: BinRange[] # categoricals can just ignore bin start and end

  FlatingMosaic:
    additionalProperties: false
    properties:
      data:
        type: MosaicData[]
      variables: VariableMapping[]

  FloatingContTablePostResponse:
    additionalProperties: false
    properties:
      mosaic:
        type: FloatingMosaic
      statsTable:
        type: ContTableStatsTable[]
        required: false