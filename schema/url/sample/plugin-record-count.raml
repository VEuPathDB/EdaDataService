#%RAML 1.0 Library

types:

  RecordCountPostRequest:
    type: VisualizationRequestBase
    properties:
      config: RecordCountSpec

  RecordCountSpec:
    additionalProperties: false
    type: object
    properties:
      entityId: string

  RecordCountPostResponse:
    additionalProperties: false
    properties:
      recordCount: integer
