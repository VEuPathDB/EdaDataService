#%RAML 1.0 Library

types:

  MapPostRequest:
    type: AnalysisRequestBase
    properties:
      config: MapSpec

  MapSpec:
    additionalProperties: false
    type: object
    properties:
      entityId: 
        type: string
      geoAggregateVariable:
        type: VariableSpec
      longitudeVariable:
        type: VariableSpec
      latitudeVariable:
        type: VariableSpec

  MapPostResponse:
    additionalProperties: false
    properties:
      geoAggregateValue:
        type: VariableSpec
      entityCount:
        type: number
      avgLat:
        type: number
      avgLon:
        type: number
      minLat:
        type: number
      minLon:
        type: number
      maxLat:
        type: number
      maxLon:
        type: number
