#%RAML 1.0 Library

types:
  # Type name determines the name of the Java class that will be generated.

  TwoByTwoStatsTable:
    additionalProperties: false
    properties:
      facetVariableDetails:
        type: StrataVariableDetails[]
        maxItems: 2
        required: false
      oddsratio:
        type: number[]
        minItems: 1
      pvalue:
        type: number[]
        minItems: 1
      orInterval:
        type: number[]
        minItems: 1
      relativerisk:
        type: number[]
        minItems: 1
      rrInterval:
        type: number[]
        minItems: 1
        
  TwoByTwoPostResponse:
    additionalProperties: false
    properties:
      mosaic:
        type: Mosaic
      sampleSizeTable:
        type: SampleSizeTable[]
      statsTable:
        type: TwoByTwoStatsTable[]